<% flash.each do |type, msg| %>
  <p><%= msg %></p>
<% end %>
<%= form_with url: "/users/", method: :post, local: true do |f|  %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <%= f.label :password_confirmation %>
  <%= f.text_field :password_confirmation %>
  <%= f.submit "Register" %>
<% end %>
